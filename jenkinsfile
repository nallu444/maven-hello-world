pipeline { 
    agent any 
    tools {
        maven 'Maven3.2.1' 
          }
    stages { 
        stage('Build') { 
                           steps {
                                   dir('/var/lib/jenkins/workspace/pipeline/my-app'){
                                   sh 'mvn clean install'
                                 }
                                 }
                       }
           }
         }

